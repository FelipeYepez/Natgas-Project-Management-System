<div id="modalAirTable" class="modal small">
    <div class="modal-content">
        <div class="row">
            <div class="col s6 offset-s3">
                <h6>Configuración AirTable</h6>
            </div>
        </div>
      <form action="/proyectos/configuracion-AirTable" method="POST">
        <input type="hidden" name="_csrf" value="<%= csrfToken %>" >
        <div class="row">
            <div class="input-field col s6 offset-s3">
                <input id="Base" type="text" class="validate" name="base" value="<%= proyecto_keys.base %>" required>
                <label for="Base">Base</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s6 offset-s3">
                <input id="User API Key" type="text" class="validate" name="API_key" value="<%= proyecto_keys.API_key %>" required>
                <label for="User API Key">User API Key</label>
            </div>
        </div>
        <div class="modal-footer">
            <button type="submit" class="waves-effect waves-green btn-flat" name="action" value="editar">Aceptar</button>
        </div>
    </form>
    </div>
  </div>

