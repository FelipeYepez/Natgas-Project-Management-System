<script>
    document.addEventListener('DOMContentLoaded', function() {
        M.AutoInit();
    });

    if("<%=title%>" == "Avance del Proyecto"){
        var element = document.getElementById("avance_proyecto");
        element.classList.add("active");
    }
    else if("<%=title%>" == "Capacidad de Equipo"){
        var element = document.getElementById("capacidad");
        element.classList.add("active");
    }
    else if("<%=title%>" == "Casos de Uso"){
        var element = document.getElementById("casos_uso");
        element.classList.add("active");
    }
    else if("<%=title%>" == "Tareas por Caso de Uso"){
        var element = document.getElementById("tareas_casos_uso");
        element.classList.add("active");
    }

</script>